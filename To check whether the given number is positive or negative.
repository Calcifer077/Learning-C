#include<stdio.h>

int main(){
	int a,b,c;
	printf("Enter three numbers.");
	scanf("%d%d%d", &a,&b,&c);
	
	if(a>b){
		if(a>c){
			printf("%d is largerr than %d and %d",a,b,c);
		}else{
			printf("%d is larger than %d and %d", c,a,b);
		}
	}if(b>c){
		if(b>a){
			printf("%d is larger than %d amd %d", b,a,c);
		}else{
			printf("%d is larger than %s and %d", c,a,b);
		}
	}
	return 0;
}
